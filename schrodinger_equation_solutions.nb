Cell[BoxData[
 RowBox[{
  RowBox[{"\[Psi]", "[", 
   RowBox[{"v_", ",", "re_", ",", "p_", ",", "\[Beta]_", ",", "r_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{
     RowBox[{"Exp", "[", 
      FractionBox[
       RowBox[{
        RowBox[{"-", "p"}], " ", 
        RowBox[{"Exp", "[", 
         RowBox[{
          RowBox[{"-", "\[Beta]"}], 
          RowBox[{"(", 
           RowBox[{"r", "-", "re"}], ")"}]}], "]"}]}], "2"], "]"}], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"p", " ", 
        RowBox[{"Exp", "[", 
         RowBox[{
          RowBox[{"-", "\[Beta]"}], 
          RowBox[{"(", 
           RowBox[{"r", "-", "re"}], ")"}]}], "]"}]}], ")"}], 
      FractionBox[
       RowBox[{"(", 
        RowBox[{"p", "-", 
         RowBox[{"2", "v"}], "-", "1"}], ")"}], "2"]], 
     RowBox[{"LaguerreL", "[", 
      RowBox[{"v", ",", 
       RowBox[{"(", 
        RowBox[{"p", "-", 
         RowBox[{"2", "v"}], "-", "1"}], ")"}], ",", 
       RowBox[{"p", " ", 
        RowBox[{"Exp", "[", 
         RowBox[{
          RowBox[{"-", "\[Beta]"}], 
          RowBox[{"(", 
           RowBox[{"r", "-", "re"}], ")"}]}], " ", "]"}]}]}], "]"}]}], ")"}], 
   "/", 
   RowBox[{"Sqrt", "[", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       UnderoverscriptBox["\[Product]", 
        RowBox[{"j", "=", "1"}], "v"], 
       RowBox[{"(", 
        RowBox[{"p", "-", 
         RowBox[{"2", "v"}], "-", "1", "+", "j"}], ")"}]}], ")"}], 
     FractionBox[
      RowBox[{"Gamma", "[", 
       RowBox[{"p", "-", 
        RowBox[{"2", "v"}], "-", "1"}], "]"}], 
      RowBox[{
       RowBox[{"v", "!"}], " ", "\[Beta]"}]]}], "]"}]}]}]], "Input",Expression\
UUID->"35445aaf-151e-473d-8eea-faea2cd2fa92"],


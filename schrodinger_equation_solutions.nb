Cell["The Harmonic Oscillator", "Section",ExpressionUUID->"c11e3cb4-2063-4985-87f9-8ba8ab4ee94b"],

Cell["\<\

We start by defining a set of functions for the un-normalized wavefunctions \
given in Table 3.1.\
\>", "Text",ExpressionUUID->"c1f3d571-4a22-4cc3-8108-0bbf7dadd0b0"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"\[CapitalPsi]0", "[", "x_", "]"}], " ", ":=", " ", 
  RowBox[{"Exp", "[", 
   RowBox[{
    RowBox[{"-", 
     SuperscriptBox["x", "2"]}], "/", "2"}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[CapitalPsi]1", "[", "x_", "]"}], " ", ":=", " ", 
  RowBox[{"2", "x", "*", 
   RowBox[{"Exp", "[", 
    RowBox[{
     RowBox[{"-", 
      SuperscriptBox["x", "2"]}], "/", "2"}], 
    "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[CapitalPsi]2", "[", "x_", "]"}], " ", ":=", " ", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{
     RowBox[{"4", 
      SuperscriptBox["x", "2"]}], "-", "2"}], ")"}], 
   RowBox[{"Exp", "[", 
    RowBox[{
     RowBox[{"-", 
      SuperscriptBox["x", "2"]}], "/", "2"}], 
    "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[CapitalPsi]3", "[", "x_", "]"}], " ", ":=", " ", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{
     RowBox[{"8", 
      SuperscriptBox["x", "3"]}], "-", 
     RowBox[{"12", "x"}]}], ")"}], 
   RowBox[{"Exp", "[", 
    RowBox[{
     RowBox[{"-", 
      SuperscriptBox["x", "2"]}], "/", "2"}], 
    "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[CapitalPsi]4", "[", "x_", "]"}], " ", ":=", " ", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{
     RowBox[{"16", 
      SuperscriptBox["x", "4"]}], "-", 
     RowBox[{"48", 
      SuperscriptBox["x", "2"]}], "+", "12"}], ")"}], 
   RowBox[{"Exp", "[", 
    RowBox[{
     RowBox[{"-", 
      SuperscriptBox["x", "2"]}], "/", "2"}], 
    "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{

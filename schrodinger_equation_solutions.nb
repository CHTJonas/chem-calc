   RowBox[{"Exp", "[", 
    RowBox[{
     RowBox[{"-", 
      SuperscriptBox["x", "2"]}], "/", "2"}], "]"}]}]}]}], "Input",ExpressionU\
UID->"55902de8-22e8-4223-9225-4904288351d9"],

Cell["\<\


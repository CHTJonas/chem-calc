   RowBox[{"\[DifferentialD]", "r"}]}]}]], "Input",ExpressionUUID->"68f42ff9-\
d065-4e3d-9749-2edf7e13c525"],

Cell["\<\

Now let us plot the 1s, 2s and 2p AOs together. We can discount negative \
values for r since they are not physical.\
\>", "Text",ExpressionUUID->"2a165fd0-09f5-4854-a6bd-9986b376e76c"],

Cell[BoxData[
 RowBox[{"Plot", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"R1s", "[", "r", "]"}], ",", " ", 
     RowBox[{"R2s", "[", "r", "]"}], ",", " ", 
     RowBox[{"R2p", "[", "r", "]"}]}], "}"}], ",", " ", 
   RowBox[{"{", 
    RowBox[{"r", ",", "0", ",", "16"}], "}"}], ",", " ", 
   RowBox[{"PlotRange", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"0", ",", "16"}], "}"}], ",", " ", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"-", "0.1"}], ",", "1"}], "}"}]}], "}"}]}], ",", " ", 
   RowBox[{"PlotStyle", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"Black", ",", " ", 
        RowBox[{"AbsoluteThickness", "[", "1", "]"}], ",", " ", 
        RowBox[{"AbsoluteDashing", "[", "1", "]"}]}], "}"}], ",", " ", 
      RowBox[{"{", 
       RowBox[{"Black", ",", " ", 
        RowBox[{"AbsoluteThickness", "[", "1", "]"}], ",", " ", 
        RowBox[{"AbsoluteDashing", "[", "2", "]"}]}], "}"}], ",", " ", 
      RowBox[{"{", 
       RowBox[{"Black", ",", " ", 
        RowBox[{"AbsoluteThickness", "[", "1", "]"}], ",", " ", 
        RowBox[{"AbsoluteDashing", "[", "3", "]"}]}], "}"}]}], "}"}]}], ",", 
   " ", 
   RowBox[{"PlotLegends", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{"\"\<1s\>\"", ",", " ", "\"\<2s\>\"", ",", " ", "\"\<2p\>\""}], 
     "}"}]}], ",", " ", 
   RowBox[{"PlotLabel", "\[Rule]", 
    RowBox[{"Style", "[", 
     RowBox[{
     "Framed", "[", 
      "\"\<Plot of the radial functions for the 1s, 2s and 2p AOs.\>\"", 
      "]"}], "]"}]}], ",", " ", 
   RowBox[{"AxesLabel", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{"\"\<r\>\"", ",", " ", "\"\<Radial Function\>\""}], "}"}]}]}], 
  "]"}]], "Input",ExpressionUUID->"09a5c759-7683-4048-8548-02ece1e3209f"],


Cell["\<\

Now we want to know the location of each radial node. However we know that \
for a radial node the wavefunction must cross zero so any spurious r=0 \
solutions returned by Mathematica can be discounted as they are not physical.\
\>", "Text",ExpressionUUID->"3cbbbaa4-7891-4cdd-aa6c-1966ab676caf"],

Cell[BoxData[
 RowBox[{"Solve", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"R1s", "[", "r", "]"}], "\[Equal]", "0"}], ",", "r"}], 
  "]"}]], "Input",ExpressionUUID->"260582e2-2768-4908-bd42-5a431e415693"],

Cell[BoxData[
 RowBox[{"Solve", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"R2s", "[", "r", "]"}], "\[Equal]", "0"}], ",", "r"}], 
  "]"}]], "Input",ExpressionUUID->"cc91d1d6-163c-470f-b282-c8a0ea5f3bcc"],

Cell[BoxData[
 RowBox[{"Solve", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"R2p", "[", "r", "]"}], "\[Equal]", "0"}], ",", "r"}], 
  "]"}]], "Input",ExpressionUUID->"e6e4b72a-f562-4987-adf0-228b973b13a5"],

Cell[BoxData[
 RowBox[{"Solve", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"R3s", "[", "r", "]"}], "\[Equal]", "0"}], ",", "r"}], 
  "]"}]], "Input",ExpressionUUID->"69aa556b-c77f-4e00-81d6-1f4c3651c729"],

Cell[BoxData[
 RowBox[{"N", "[", 
  RowBox[{"Solve", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"R3s", "[", "r", "]"}], "\[Equal]", "0"}], ",", "r"}], "]"}], 
  "]"}]], "Input",
 NumberMarks->False,ExpressionUUID->"edfec852-b826-4cc5-b9a2-a38cb96425a8"],

Cell[BoxData[
 RowBox[{"Solve", "[", 

518c-4e00-bec0-cd6bf314b744"],

Cell[BoxData[
 RowBox[{
  SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
  RowBox[{
   RowBox[{"P3s", "[", "r", "]"}], 
   RowBox[{"\[DifferentialD]", "r"}]}]}]], "Input",ExpressionUUID->"68a45469-\
1e5f-4136-a8f0-76c0e6100a52"],

Cell[BoxData[
 RowBox[{
  SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
  RowBox[{
   RowBox[{"P3p", "[", "r", "]"}], 
   RowBox[{"\[DifferentialD]", "r"}]}]}]], "Input",ExpressionUUID->"ac34284d-\
4445-4a77-a382-9c97b6e378b3"],

Cell[BoxData[
 RowBox[{
  SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 

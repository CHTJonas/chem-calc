Cell[CellGroupData[{

Cell["Contour plots of atomic orbitals", "Section",ExpressionUUID->"2152bbc8-f2c7-402b-ad7e-eedf0af624c0"],

Cell["\<\

Once again we begin by defining some functions that are more suited to our \
needs.\
\>", "Text",ExpressionUUID->"a86fd093-60eb-4d85-83fe-a6a8fce0a4bd"],

Cell[BoxData[
 RowBox[{
  RowBox[{"ContPlotXY", "[", 
   RowBox[{
   "f_", ",", "rm_", ",", "npts_", ",", "cmin_", ",", "cmax_", ",", "ncont_", 
    ",", "title_"}], "]"}], ":=", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"npts", "<", "5"}], ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Print", "[", "\"\<number of points must be >4\>\"", "]"}], 
        ",", 
        RowBox[{"Abort", "[", "]"}]}], "}"}], ",", 
      RowBox[{"{", "}"}]}], "]"}], ";", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"cmax", "<", "0.0"}], " ", "||", 
       RowBox[{"cmin", "<", "0.0"}]}], " ", ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
        "Print", "[", "\"\<contour levels must be specified as positive\>\"", 
         "]"}], ",", 
        RowBox[{"Abort", "[", "]"}]}], "}"}], ",", 
      RowBox[{"{", "}"}]}], "]"}], ";", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"cmax", "<=", "cmin"}], ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
        "Print", "[", 
         "\"\<max contour level must be greater than minimum level\>\"", 
         "]"}], ",", 
        RowBox[{"Abort", "[", "]"}]}], "}"}], ",", 
      RowBox[{"{", "}"}]}], "]"}], ";", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"ncont", "<", "2"}], ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Print", "[", "\"\<number of countours must be >1\>\"", "]"}],
         ",", 
        RowBox[{"Abort", "[", "]"}]}], "}"}], ",", 
      RowBox[{"{", "}"}]}], "]"}], ";", 
    RowBox[{"inc", "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"cmax", "-", "cmin"}], ")"}], "/", 
      RowBox[{"(", 
       RowBox[{"ncont", "-", "1"}], ")"}]}]}], ";", 
    RowBox[{"contlevs", "=", 
     RowBox[{"Join", "[", 
      RowBox[{
       RowBox[{"Table", "[", 
        RowBox[{"lev", ",", 
         RowBox[{"{", 
          RowBox[{"lev", ",", 
           RowBox[{"-", "cmax"}], ",", 
           RowBox[{"-", "cmin"}], ",", "inc"}], "}"}]}], "]"}], ",", 
       RowBox[{"{", "0.0", "}"}], ",", 
       RowBox[{"Table", "[", 
        RowBox[{"lev", ",", 
         RowBox[{"{", 
          RowBox[{"lev", ",", "cmin", ",", "cmax", ",", "inc"}], "}"}]}], 
        "]"}]}], "]"}]}], ";", 
    RowBox[{"contsty", "=", 
     RowBox[{"Join", "[", 
      RowBox[{
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"GrayLevel", "[", "0.1", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "1", ",", "ncont"}], "}"}]}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"{", 
         RowBox[{"Black", ",", 
          RowBox[{"AbsoluteDashing", "[", "2", "]"}]}], "}"}], "}"}], ",", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"GrayLevel", "[", "0.6", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "1", ",", "ncont"}], "}"}]}], "]"}]}], "]"}]}], 
    ";", 
    RowBox[{"ContourPlot", "[", 
     RowBox[{
      RowBox[{"f", "/.", 
       RowBox[{"z", "->", "0"}]}], ",", 
      RowBox[{"{", 
       RowBox[{"x", ",", 
        RowBox[{"-", "rm"}], ",", "rm"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"y", ",", 
        RowBox[{"-", "rm"}], ",", "rm"}], "}"}], ",", 
      RowBox[{"ContourShading", "\[Rule]", "False"}], ",", 
      RowBox[{"Contours", "\[Rule]", "contlevs"}], ",", 
      RowBox[{"ContourStyle", "\[Rule]", "contsty"}], ",", 
      RowBox[{"FrameLabel", "\[Rule]", 
       RowBox[{"{", 
        RowBox[{
        "\"\<\!\(\*StyleBox[\"x\",FontSlant->\"Italic\"]\) / Bohr\>\"", ",", 
         "\"\<\!\(\*StyleBox[\"y\",FontSlant->\"Italic\"]\) / Bohr\>\""}], 
        "}"}]}], ",", 
      RowBox[{"PlotLabel", "\[Rule]", 
       RowBox[{"Style", "[", 
        RowBox[{"Framed", "[", "title", "]"}], "]"}]}], ",", 
      RowBox[{"PlotPoints", "\[Rule]", "npts"}], ",", 
      RowBox[{"PlotRange", "\[Rule]", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"-", "rm"}], ",", "rm"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"-", "rm"}], ",", "rm"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"-", "1"}], ",", "1"}], "}"}]}], "}"}]}]}], "]"}]}], 
   ")"}]}]], "Input",ExpressionUUID->"258d78da-2e14-4580-a534-f4916dcdd2c9"],


Notebook[{

Cell[CellGroupData[{
Cell["Visualizing Atomic Orbitals", "Title",ExpressionUUID->"c1515cd3-e4ca-4cbf-9d07-09378f571fab"],

Cell[CellGroupData[{

Cell["Plots of the radial parts of the atomic orbitals", "Section",ExpressionUUID->"3adc33ff-a77a-4ca9-9755-47e1b0b6ccdc"],

Cell["\<\

We begin by defining functions for the radial parts of the AOs of a hydrogen \
atom.\
\>", "Text",ExpressionUUID->"1e9ebe5e-f98a-4213-974c-d094a6acd4a4"],

Cell[BoxData[
 RowBox[{
  RowBox[{"R1s", "[", "r_", "]"}], ":=", 
  RowBox[{"2", 
   RowBox[{"Exp", "[", 
    RowBox[{"-", "r"}], "]"}]}]}]], "Input",ExpressionUUID->"0791d751-803a-\
42bc-8ccf-140d793b7103"],

Cell[BoxData[
 RowBox[{
  RowBox[{"R2s", "[", "r_", "]"}], ":=", 
  RowBox[{
   FractionBox["1", 
    RowBox[{"2", 
     SqrtBox["2"]}]], 
   RowBox[{"(", 
    RowBox[{"2", "-", "r"}], ")"}], 
   RowBox[{"Exp", "[", 
    RowBox[{
     RowBox[{"-", "r"}], "/", "2"}], "]"}]}]}]], "Input",ExpressionUUID->\
"4db3ce22-3940-496e-bd3b-05fef7c673c9"],

Cell[BoxData[
 RowBox[{
  RowBox[{"R2p", "[", "r_", "]"}], ":=", 
  RowBox[{
   FractionBox["1", 
    RowBox[{"2", 
     SqrtBox["6"]}]], 
   RowBox[{"(", "r", ")"}], 
   RowBox[{"Exp", "[", 
    RowBox[{
     RowBox[{"-", "r"}], "/", "2"}], "]"}]}]}]], "Input",ExpressionUUID->\
"0e7bce25-90fc-49a0-aa00-6032c786d206"],

Cell[BoxData[
 RowBox[{
  RowBox[{"R3s", "[", "r_", "]"}], ":=", 
  RowBox[{
   FractionBox["2", 
    RowBox[{"81", 
     SqrtBox["3"]}]], 
   RowBox[{"(", 
    RowBox[{"27", "-", 
     RowBox[{"18", "r"}], "+", 
     RowBox[{"2", 
      SuperscriptBox["r", "2"]}]}], ")"}], 
   RowBox[{"Exp", "[", 
    RowBox[{
     RowBox[{"-", "r"}], "/", "3"}], "]"}]}]}]], "Input",ExpressionUUID->\
"aeea2b2e-1c03-4762-b542-ea146e425d50"],

Cell[BoxData[
 RowBox[{
  RowBox[{"R3p", "[", "r_", "]"}], ":=", 
  RowBox[{
   FractionBox[
    RowBox[{"2", 
     SqrtBox["2"]}], 
    RowBox[{"81", 
     SqrtBox["3"]}]], 
   RowBox[{"(", 
    RowBox[{"6", "-", "r"}], ")"}], "r", "*", 
   RowBox[{"Exp", "[", 
    RowBox[{
     RowBox[{"-", "r"}], "/", "3"}], "]"}]}]}]], "Input",ExpressionUUID->\
"7384f3e2-c1d5-406f-8cba-837b590b87ea"],

Cell[BoxData[
 RowBox[{
  RowBox[{"R3d", "[", "r_", "]"}], ":=", 
  RowBox[{
   FractionBox[
    RowBox[{"2", 
     SqrtBox["2"]}], 
    RowBox[{"81", 
     SqrtBox["15"]}]], 
   RowBox[{"(", 
    SuperscriptBox["r", "2"], ")"}], 
   RowBox[{"Exp", "[", 
    RowBox[{
     RowBox[{"-", "r"}], "/", "3"}], "]"}]}]}]], "Input",ExpressionUUID->\
"51b081e4-ea90-4631-91ae-2718d6226fa2"],

Cell["\<\

We then define equivalent functions for the RDF.\
\>", "Text",ExpressionUUID->"0a259070-33c1-4db7-a149-f4145af1d26c"],

Cell[BoxData[
 RowBox[{
